


  <h1>Realtors</h1>

  <h3>********Listing Realtors**********</h3>
  <br>
  <div class="row">
    <div class ="col-md-2">
     Name
    </div>
    <div class ="col-md-2">
     E-mail
    </div>
    <div class ="col-md-2">
     Company Id
    </div>
    <div class ="col-md-2">
      Phone Number
    </div>




  </div>

  <hr>
  <% @realtors.each do |realtor| %>
    <div class="row">
      <div class ="col-md-2"><%= realtor.name %></div>|
        <div class ="col-md-2"><%= realtor.email %></div>|
          <div class ="col-md-2"><%= realtor.companyId %></div>|
            <div class ="col-md-2"><%= realtor.phoneNumber %></div>|

              <div class ="col-md-2"> <%= link_to 'Edit', edit_realtor_path(realtor) %> /
               <%= link_to 'Destroy', realtor, method: :delete, data: { confirm: 'Are you sure?' } %></div>

    </div>
    <hr>
  <% end %>


<br>

<%= link_to 'Add Realtor', { :action =>"new"} , :class => "btn btn-primary"%> <br/><br/>